  <make>ITpp.teste_general_par3($qtd_in, $rep, $last_value)</make>

  <param>
    <name>Quantidade Entrada</name>
    <key>qtd_in</key>
    <type>int</type>
  </param>

  <param>
    <name>Repetição</name>
    <key>rep</key>
    <type>int</type>
  </param>

  <param>
    <name>Last Value</name>
    <key>last_value</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
